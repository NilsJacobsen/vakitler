{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "short_name": "Vakitler",
  "name": "Namaz Vakitleri",
  "description": "Namaz vakitlerini kolay şekilde öğrenin.",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone",
  "orientation": "portrait-primary",
  "scope": "/",
  "start_url": "/",
  "start_URL": "/",
  "icons": [
    {
      "src": "/icons-vector.svg",
      "type": "image/svg+xml",
      "sizes": "512x512"
    },
    {
      "src": "/icons-192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/icons-512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ]
}
